# TSR Generation Environment Configuration
# Copy this file to .env and customize the values

# API Configuration
GEMINI_API_KEY=AIzaSyCMWv6_2vbjSqE7ot900sKz85oWrjEM2wA
CREWAI_VERBOSE=true
CREWAI_DEBUG=false

# Quality Gate Configuration
# Override quality gate values using environment variables

# Global Quality Gate Overrides
TSR_QUALITY_GATE=strict

# Default Quality Gate - Release Thresholds
TSR_DEFAULT_APPROVED_PASS_RATE=90
TSR_DEFAULT_CONDITIONAL_PASS_RATE=75
TSR_DEFAULT_APPROVED_CRITICAL_DEFECTS=0
TSR_DEFAULT_CONDITIONAL_CRITICAL_DEFECTS=0
TSR_DEFAULT_APPROVED_MAJOR_DEFECTS=0
TSR_DEFAULT_CONDITIONAL_MAJOR_DEFECTS=2

# Strict Quality Gate - Release Thresholds
TSR_STRICT_APPROVED_PASS_RATE=98
TSR_STRICT_CONDITIONAL_PASS_RATE=95
TSR_STRICT_APPROVED_CRITICAL_DEFECTS=0
TSR_STRICT_CONDITIONAL_CRITICAL_DEFECTS=0
TSR_STRICT_APPROVED_MAJOR_DEFECTS=0
TSR_STRICT_CONDITIONAL_MAJOR_DEFECTS=1

# Lenient Quality Gate - Release Thresholds
TSR_LENIENT_APPROVED_PASS_RATE=80
TSR_LENIENT_CONDITIONAL_PASS_RATE=50
TSR_LENIENT_APPROVED_CRITICAL_DEFECTS=0
TSR_LENIENT_CONDITIONAL_CRITICAL_DEFECTS=1
TSR_LENIENT_APPROVED_MAJOR_DEFECTS=2
TSR_LENIENT_CONDITIONAL_MAJOR_DEFECTS=5

# Custom Quality Gate - Release Thresholds
TSR_CUSTOM_APPROVED_PASS_RATE=85
TSR_CUSTOM_CONDITIONAL_PASS_RATE=70
